///getTypeColor(TYPE_ID)

switch (argument[0]){
    default:
        return c_aqua;
        break;
        
    case TYPE_DIRT:
        return c_maroon;
        break;
        
    case TYPE_WATER:
        return c_blue;
        break;
        
    case TYPE_VOID:
        return c_black;
        break;
    
}
